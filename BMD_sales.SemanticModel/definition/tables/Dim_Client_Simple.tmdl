					USERELATIONSHIP(Dim_User[BazaarID], 'Dim_Client_Simple'[BazaarID]),
					'Dim_Client_Simple'[BazaarID] <> -1
				)
			VAR NationalClients = 
				CALCULATE(
					DISTINCTCOUNT('Dim_Client_Simple'[ClientID]),
					'Dim_Client_Simple'[BazaarID] = -1
				)
			RETURN
				IF(ISBLANK(UserBazaar), BLANK(), LocalClients + NationalClients)
