table Pending
	lineageTag: 07fb456b-ba79-449d-8465-79f212846f8b

	measure Pending =
			
			CALCULATE (
			    [Total_Order_Bags] * 3,
			    NOT ISBLANK ( Dim_Contractor[ContractorID] )
			)
		formatString: 0
		lineageTag: 3473f589-1538-4b3a-874d-7d5259a9f104

	measure Pending_For_SR = ```
			
			VAR Result =
			    CALCULATE (
			        [Total_PartnerEligible_Orders_Bags] * 3,
			        NOT ISBLANK ( Dim_Contractor[ContractorID] ),
			        Dim_User[RoleName] = "Sr",
			        Fact_UserOrders[order_status] <> "APPROVED",
			        Fact_UserOrders[order_status] <> "REJECTED"
			    )
			RETURN
			IF ( ISBLANK ( Result ), 0, Result )
			
			```
		formatString: 0
		lineageTag: e4a262ff-9a94-45f1-b7f7-7e2d91a11750

	measure Due_For_SR =
			
			VAR Result =
			    CALCULATE (
			        [Total_PartnerEligible_Orders_Bags] * 3,
			        NOT ISBLANK ( Dim_Contractor[ContractorID] ),
			        Dim_User[RoleName] = "Sr",
			    Fact_UserOrders[order_status] = "APPROVED"
			    )
			RETURN
			IF (
			    ISBLANK ( Result ),
			    0,
			    Result
			)
		formatString: 0
		lineageTag: 1ad26a09-03a8-430b-be5e-c0b0030cfac8

	measure Remaining_Due_For_SR = ```
			
			VAR Remaining =
			    [Due_For_SR]
			    - [Processing_Raw]
			    - [Net_Paid]
			RETURN
			IF ( Remaining <= 0, 0, Remaining )
			
			```
		formatString: 0
		lineageTag: 4d1cc3cc-bea6-4371-8bf3-6004f506c02b

	measure Total_Pending_For_SR = ```
			
