			```
		formatString: 0
		lineageTag: baf5df4b-729f-4ec4-865d-aaf135c93f11

	measure Partner_Incentive_Rate = ```
			
			VAR Bags = [Total_PartnerEligible_Orders_Bags]
			RETURN
			SWITCH (
			    TRUE(),
			    Bags >= 1000, 8.00,
			    Bags >= 700,  6.00,
			    Bags >= 500,  5.00,
			    Bags >= 300,  4.00,
			    Bags >= 200,  3.50,
			    Bags >= 100,  3.00,
			    Bags >= 50,   2.50,
			    0
			)
			
			```
		lineageTag: 784d7d89-46eb-4ebf-931a-742075338a23

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Pending_For_SR_New = ```
			
			VAR Amount =
			    CALCULATE (
			        [Total_PartnerEligible_Orders_Bags]
			            * [Partner_Incentive_Rate],
			        NOT ISBLANK ( Dim_Contractor[ContractorID] ),
			        Dim_User[RoleName] = "Sr",
			        Fact_UserOrders[order_status] <> "APPROVED",
			        Fact_UserOrders[order_status] <> "REJECTED"
			    )
