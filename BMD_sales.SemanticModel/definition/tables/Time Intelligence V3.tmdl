table 'Time Intelligence V3'
	lineageTag: 029a87d5-03f8-4ea0-93f6-c5639c6d56b4

	calculationGroup
		precedence: 11

		calculationItem Today =
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS( Dim_Date[DateKey] = TODAY() )
				)

		calculationItem Yesterday =
				CALCULATE(
				    SELECTEDMEASURE(),
				    Dim_Date[Date] = TODAY() - 1
				)

		calculationItem WTD =
				VAR _Today = TODAY()
				VAR _WeekStart = _Today - ( WEEKDAY(_Today, 2) - 1 )
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS( Dim_Date[DateKey] >= _WeekStart ),
				    KEEPFILTERS( Dim_Date[DateKey] <= _Today )
				)

		calculationItem MTD =
				VAR _Today = TODAY()
				VAR _MonthStart = DATE( YEAR(_Today), MONTH(_Today), 1 )
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS( Dim_Date[DateKey] >= _MonthStart ),
				    KEEPFILTERS( Dim_Date[DateKey] <= _Today )
				)

		calculationItem YTD =
				VAR _Today = TODAY()
				VAR _YearStart = DATE( YEAR(_Today), 1, 1 )
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS( Dim_Date[DateKey] >= _YearStart ),
				    KEEPFILTERS( Dim_Date[DateKey] <= _Today )
				)

		calculationItem 'Last 7' =
				VAR _Today = TODAY()
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS( Dim_Date[DateKey] >= _Today - 6 ),
				    KEEPFILTERS( Dim_Date[DateKey] <= _Today )
				)

		calculationItem 'Last 30' =
				VAR _Today = TODAY()
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    KEEPFILTERS( Dim_Date[DateKey] >= _Today - 29 ),
				    KEEPFILTERS( Dim_Date[DateKey] <= _Today )
				)

