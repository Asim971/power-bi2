{
    "objectType": "page",
    "name": "Customers Dashboard",
    "displayName": "Customers Dashboard",
    "displayOption": "FitToPage",
    "width": 1920,
    "height": 1080,
    "ordinal": 12,
    "config": {
        "background": {
            "color": "#F8F9FA",
            "transparency": 0
        },
        "visibility": "visible",
        "pageFilter": {
            "enabled": true,
            "filter": {
                "table": "Dim_Client_Simple",
                "column": "EntityGroup",
                "values": [
                    "Customers"
                ]
            }
        }
    },
    "visuals": [
        {
            "name": "Header_Customers",
            "type": "textbox",
            "x": 0,
            "y": 0,
            "width": 1920,
            "height": 60,
            "config": {
                "textbox": {
                    "paragraphs": [
                        {
                            "textRuns": [
                                {
                                    "value": "üè† CUSTOMERS DASHBOARD    |    Sites & Individual Home Builders Performance",
                                    "textStyle": {
                                        "fontFamily": "Segoe UI Semibold",
                                        "fontSize": 18,
                                        "fontColor": "#FFFFFF"
                                    }
                                }
                            ]
                        }
                    ],
                    "backgroundColor": "#E67E22"
                }
            }
        },
        {
            "name": "Last_Refresh_Label_Cust",
            "type": "card",
            "x": 1600,
            "y": 10,
            "width": 300,
            "height": 40,
            "config": {
                "title": "Last Refresh",
                "dataField": "Fact_Visit[Last_Refresh_DateTime]",
                "fontSize": 10,
                "backgroundColor": "transparent"
            }
        },
        {
            "name": "KPI_Total_Visit_Customers",
            "type": "card",
            "x": 40,
            "y": 80,
            "width": 280,
            "height": 120,
            "config": {
                "title": "TOTAL VISITS",
                "dataField": "Fact_Visit[Total_Visit_FRD]",
                "format": "#,##0",
                "fontSize": 36,
                "fontColor": "#E67E22",
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8,
                    "color": "#E2E8F0"
                },
                "shadow": true,
                "subtitle": "Customers (Site + IHB)"
            }
        },
        {
            "name": "KPI_Average_Visit_Customers",
            "type": "card",
            "x": 340,
            "y": 80,
            "width": 280,
            "height": 120,
            "config": {
                "title": "AVERAGE VISIT",
                "dataField": "Fact_Visit[Average_Visit_FRD]",
                "format": "#,##0",
                "fontSize": 36,
                "fontColor": "#27AE60",
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8,
                    "color": "#E2E8F0"
                },
                "shadow": true,
                "subtitle": "Per Employee"
            }
        },
        {
            "name": "KPI_MoM_Variance_Cust",
            "type": "card",
            "x": 640,
            "y": 80,
            "width": 200,
            "height": 120,
            "config": {
                "title": "MoM VARIANCE",
                "dataField": "Fact_Visit[MoM_Variance]",
                "format": "+#,##0;-#,##0;0",
                "fontSize": 28,
                "conditionalFormatting": {
                    "positive": "#27AE60",
                    "negative": "#E74C3C",
                    "zero": "#95A5A6"
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "KPI_MoM_Variance_Pct_Cust",
            "type": "card",
            "x": 860,
            "y": 80,
            "width": 200,
            "height": 120,
            "config": {
                "title": "MoM %",
                "dataField": "Fact_Visit[MoM_Variance_Pct]",
                "format": "0.0%",
                "fontSize": 28,
                "conditionalFormatting": {
                    "positive": "#27AE60",
                    "negative": "#E74C3C"
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Chart_Visit_Trend_Cust",
            "type": "lineChart",
            "x": 40,
            "y": 220,
            "width": 900,
            "height": 300,
            "config": {
                "title": "üìà VISIT TREND BY DESIGNATION",
                "xAxis": {
                    "field": "Dim_Date[YearMonth]",
                    "title": "Month"
                },
                "yAxis": {
                    "title": "Visit Count"
                },
                "series": [
                    {
                        "name": "All Visits",
                        "field": "Fact_Visit[Visits_Customers]",
                        "color": "#E67E22"
                    }
                ],
                "legend": {
                    "show": true,
                    "position": "top",
                    "field": "Dim_User[Designation]"
                },
                "tooltip": {
                    "fields": [
                        "Dim_Date[YearMonth]",
                        "Dim_User[Designation]",
                        "Fact_Visit[Visits_Customers]"
                    ]
                },
                "dataLabels": {
                    "show": false
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Chart_MoM_Comparison_Cust",
            "type": "clusteredBarChart",
            "x": 960,
            "y": 220,
            "width": 920,
            "height": 300,
            "config": {
                "title": "üìä MoM COMPARISON BY LOCATION",
                "yAxis": {
                    "field": "Dim_Territory[AreaName]",
                    "title": "Location"
                },
                "xAxis": {
                    "title": "Visit Count"
                },
                "series": [
                    {
                        "name": "Current Month",
                        "field": "Fact_Visit[Visits_Current_Month]",
                        "color": "#E67E22"
                    },
                    {
                        "name": "Previous Month",
                        "field": "Fact_Visit[Visits_Previous_Calendar_Month]",
                        "color": "#F5CBA7"
                    }
                ],
                "legend": {
                    "show": true,
                    "position": "top"
                },
                "sort": {
                    "field": "Fact_Visit[Visits_Current_Month]",
                    "direction": "descending"
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Chart_Visit_By_Location_Cust",
            "type": "barChart",
            "x": 40,
            "y": 540,
            "width": 600,
            "height": 280,
            "config": {
                "title": "üó∫Ô∏è VISITS BY LOCATION",
                "yAxis": {
                    "field": "Dim_Territory[ZoneName]",
                    "title": "Location"
                },
                "xAxis": {
                    "field": "Fact_Visit[Visits_Customers]",
                    "title": "Visits"
                },
                "sort": {
                    "field": "Fact_Visit[Visits_Customers]",
                    "direction": "descending"
                },
                "topN": 10,
                "color": "#F39C12",
                "dataLabels": {
                    "show": true
                },
                "drillThrough": {
                    "enabled": true,
                    "targetPage": "Visit Detail"
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Chart_Top_10_Performers_Cust",
            "type": "funnel",
            "x": 660,
            "y": 540,
            "width": 430,
            "height": 280,
            "config": {
                "title": "üèÜ TOP 10 PERFORMERS",
                "category": {
                    "field": "Dim_User[EmployeeName]"
                },
                "values": {
                    "field": "Fact_Visit[Employee_Visit_Count]"
                },
                "filter": {
                    "field": "Fact_Visit[Is_Top_10]",
                    "value": 1
                },
                "sort": {
                    "field": "Fact_Visit[Employee_Visit_Count]",
                    "direction": "descending"
                },
                "colors": [
                    "#27AE60",
                    "#2ECC71",
                    "#58D68D",
                    "#82E0AA",
                    "#ABEBC6",
                    "#D5F5E3",
                    "#E8F8F5",
                    "#F0FDF9",
                    "#F5FFFA",
                    "#FAFFFE"
                ],
                "showPercentages": true,
                "drillThrough": {
                    "enabled": true,
                    "targetPage": "Visit Detail"
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Chart_Location_Breakdown_Cust",
            "type": "columnChart",
            "x": 1110,
            "y": 540,
            "width": 430,
            "height": 280,
            "config": {
                "title": "üìç LOCATION BREAKDOWN",
                "xAxis": {
                    "field": "Dim_Territory[RegionName]",
                    "title": "Territory"
                },
                "yAxis": {
                    "field": "Fact_Visit[Visits_Customers]",
                    "title": "Visits"
                },
                "color": "#FAD7A0",
                "sort": {
                    "field": "Fact_Visit[Visits_Customers]",
                    "direction": "descending"
                },
                "dataLabels": {
                    "show": true
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Chart_Bottom_10_Performers_Cust",
            "type": "funnel",
            "x": 1560,
            "y": 540,
            "width": 340,
            "height": 280,
            "config": {
                "title": "‚ö†Ô∏è BOTTOM 10 PERFORMERS",
                "category": {
                    "field": "Dim_User[EmployeeName]"
                },
                "values": {
                    "field": "Fact_Visit[Employee_Visit_Count]"
                },
                "filter": {
                    "field": "Fact_Visit[Is_Bottom_10]",
                    "value": 1
                },
                "sort": {
                    "field": "Fact_Visit[Employee_Visit_Count]",
                    "direction": "ascending"
                },
                "colors": [
                    "#E74C3C",
                    "#EC7063",
                    "#F1948A",
                    "#F5B7B1",
                    "#FADBD8",
                    "#FDEDEC",
                    "#FEF5F5",
                    "#FFF8F8",
                    "#FFFAFA",
                    "#FFFFFF"
                ],
                "showPercentages": true,
                "drillThrough": {
                    "enabled": true,
                    "targetPage": "Visit Detail"
                },
                "backgroundColor": "#FFFFFF",
                "border": {
                    "radius": 8
                }
            }
        },
        {
            "name": "Filter_Panel_Customers",
            "type": "slicerPanel",
            "x": 1080,
            "y": 80,
            "width": 800,
            "height": 120,
            "config": {
                "layout": "horizontal",
                "slicers": [
                    {
                        "title": "üìÖ Date Filter",
                        "field": "DateFilterParam[FilterOption]",
                        "type": "radioButton",
                        "default": "MTD",
                        "options": [
                            "Daily",
                            "WTD",
                            "MTD",
                            "YTD"
                        ]
                    },
                    {
                        "title": "üó∫Ô∏è Location Level",
                        "field": "LocationLevelParam[LevelOption]",
                        "type": "radioButton",
                        "default": "Zone",
                        "options": [
                            "Area",
                            "Zone",
                            "Territory"
                        ]
                    },
                    {
                        "title": "üè¢ Company",
                        "field": "Fact_Visit[CompanyCode]",
                        "type": "checkbox",
                        "default": [
                            "ACL",
                            "AIL"
                        ]
                    },
                    {
                        "title": "üë§ Employee",
                        "field": "Dim_User[EmployeeName]",
                        "type": "dropdown",
                        "default": "All"
                    }
                ]
            }
        }
    ],
    "interactions": [
        {
            "source": "Chart_Visit_By_Location_Cust",
            "target": [
                "Chart_Visit_Trend_Cust",
                "Chart_Top_10_Performers_Cust",
                "Chart_Bottom_10_Performers_Cust"
            ],
            "type": "filter"
        },
        {
            "source": "Chart_Top_10_Performers_Cust",
            "target": [
                "Chart_Visit_Trend_Cust",
                "Chart_MoM_Comparison_Cust"
            ],
            "type": "highlight"
        }
    ],
    "drillThrough": {
        "enabled": true,
        "fields": [
            "Dim_Territory[AreaName]",
            "Dim_User[EmployeeID]"
        ],
        "targetPage": "Visit Detail"
    },
    "export": {
        "enabled": true,
        "formats": [
            "Excel",
            "CSV",
            "PDF"
        ]
    },
    "bookmarks": [
        {
            "name": "CustomersDefault",
            "displayName": "Customers Overview",
            "description": "Default view for Customers Dashboard (Sites & IHB)",
            "isDefault": true
        }
    ]
}